<Application x:Class="SpellWork.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:s="clr-namespace:SpellWork"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Controls/SpellView/ItemsView.xaml"/>
                <ResourceDictionary Source="Controls/SpellView/SpellEffectView.xaml"/>
                <ResourceDictionary Source="Controls/SpellView/SpellAttributesView.xaml" />
                <ResourceDictionary Source="Misc.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
